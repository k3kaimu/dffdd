{
    "name": "dffdd",
    "description": "Digital Filter Library for Full Duplex writted in D",
    "copyright": "Copyright Â© 2015, Kazuki KOMATSU",
    "authors": ["Kazuki Komatsu"],
    "targetType": "library",

    "__foo": "/usr/local/opt/openblas/lib",
    "libs": ["openblas", "itpp", "stdc++", "fftw3"],
    "lflags-osx": ["-L/usr/local/opt/openblas/lib"],
    "lflags-linux": ["-L/opt/OpenBLAS/lib"],

    "dependencies": {
        "carbon": {"path": "../carbon"},
        "dranges": {"path": "../dranges"},
        "dfftw3": "~>1.0.1"
    },
    "subConfigurations": {
        "pyd": "python27",
    },

    "configurations":[
        {
            "name": "library"
        },

        {
            "name": "test",
            "sourceFiles": ["ditpp.o"],
        },

        {
            "name": "preBuildCopyCommand",
            "copyFiles": ["cpp/ditpp.cpp"],
        },

        {
            "name": "preBuildPostCopyCommand",
            "preGenerateCommands": ["g++ -std=c++14 -c ditpp.cpp > gcc_out.txt"]
        }
    ]
}
